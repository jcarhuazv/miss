<p><%= t(:votes, :count => member.votings.count) %></p>

<% if @contest.during_voting? %>
  <% if Voting.can_vote?(request) %>
    <%= form_tag voting_path(@contest, member), :method => :post, :class => "voting_form" do %>
      <p><%= submit_tag "Голосовать", :class => "button red" %></p>
    <% end %>
  <% end %>
<% end %>

